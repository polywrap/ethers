{
  "name": "@polywrap/ethereum-wrapper",
  "description": "Ethereum Wrapper that can be invoked from any language with a Polywrap client",
  "version": "0.1.1",
  "license": "MIT",
  "main": "build/index.js",
  "files": ["build"],
  "scripts": {
    "test:codegen": "polywrap codegen -m tests/types/polywrap.app.yaml -g tests/types/wrap",
    "test:e2e" : "jest --passWithNoTests --runInBand --verbose",
    "test": "yarn test:codegen && yarn test:e2e",
    "codegen": "npx polywrap codegen",
    "build": "npx polywrap build -s image --codegen",
    "deploy": "npx polywrap deploy -o deployment.json"
  },
  "devDependencies": {
    "ethereum-provider-js": "0.2.1",
    "@polywrap/client-js": "0.10.0-pre.7",
    "@polywrap/core-js": "0.10.0-pre.7",
    "@polywrap/test-env-js": "0.10.0-pre.7",
    "@types/jest": "27.0.3",
    "axios": "0.27.2",
    "eth-ens-namehash": "2.0.8",
    "ethers": "5.7.2",
    "jest": "29.0.3",
    "js-sha3": "0.8.0",
    "polywrap": "0.10.0-pre.7",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
